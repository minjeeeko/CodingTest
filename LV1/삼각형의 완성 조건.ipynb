   "outputs": [],
   "source": [
    "def solution(sides):\n",
    "    sides.sort()\n",
    "    if sides[2] >= (sides[0] + sides[1]):\n",
    "        answer = 2\n",
    "    else: \n",
    "        answer = 1\n",
    "    return answer"
   ]
  },
